<script>
	function setActiveTab(activeTabId)
	{		
		/******** to active personal info start ***********/
		var my_active_arr = new Array( "1", "2","3" );
		
		for ( var i = 0; i < my_active_arr.length; i++ ) {
			if(my_active_arr[i] == activeTabId )
			{
				$("#profile_li_"+my_active_arr[i]).addClass('active');
				$("#tab_"+my_active_arr[i]+"-"+my_active_arr[i]).addClass('active');
			}
			else
			{
				$("#profile_li_"+my_active_arr[i]).removeClass('active');
				$("#tab_"+my_active_arr[i]+"-"+my_active_arr[i]).removeClass('active');
			}
		}
		/******** to active personal info end ***********/
		
		var path = "{{path('profile_activetab')}}";
        $.ajax({
            url: path,
            type: 'POST',
            data: 'activeTabId='+activeTabId,
            success: function(response) {
            }
        });
	}
</script>

